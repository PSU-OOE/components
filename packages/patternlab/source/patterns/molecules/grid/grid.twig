{% set items = [] %}
{% for item in 1..9 %}
  {% set rendered_item %}
    <div style="padding:1.5rem 2rem;background:blue;color:white">
      Item {{ item }}
    </div>
  {% endset %}
  {% set items = items|merge([rendered_item]) %}
{% endfor %}

<div style="display:flex;flex-flow:row wrap;row-gap:3rem;max-width:119rem;padding:4rem;">
  <div style="flex: 1 1 0;">
    {% include '@molecules/grid/grid.twig' with {
      items: items,
    } only %}
  </div>
</div>
