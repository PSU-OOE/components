{% set form %}
  <form action="/action_page.php">
    <label for="intro_form_fname_5">First name:</label><br>
    <input type="text" id="intro_form_fname_5" name="intro_form_fname_5"><br><br>
    <label for="intro_form_lname_5">Last name:</label><br>
    <input type="text" id="intro_form_lname_5" name="intro_form_lname_5"><br><br>
    <label for="intro_form_email_5">Email:</label><br>
    <input type="email" id="intro_form_email_5" name="intro_form_email_5"><br><br>
    <label for="intro_form_phone_5">Phone</label><br>
    <input type="tel" id="intro_form_phone_5" name="intro_form_phone_5"><br><br>
    {% include '@psu-ooe/button/button.twig' with {
      type: 'submit',
      label: 'Submit',
    } only %}
  </form>
{% endset %}
{% include '@psu-ooe/intro-form/intro-form.twig' with {
  "heading_level": "h2",
  "title": "Ready to Learn More?",
  "images": [
    "<img src=\"https://via.placeholder.com/800x350\" widith=\"800\" height=\"350\" alt=\"\">",
    "<img src=\"https://via.placeholder.com/1600x700\" width=\"1600\" height=\"700\" alt=\"\">",
  ],
  "description": "Get the resources you need to make informed decisions about your education. Request information on this program and other programs of interest by completing this form.",
  "form": form
} only %}
