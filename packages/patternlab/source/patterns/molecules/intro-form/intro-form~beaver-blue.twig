{% set form %}
  <form action="/action_page.php">
    <label for="intro_form_fname_2">First name:</label><br>
    <input type="text" id="intro_form_fname_2" name="intro_form_fname_2"><br><br>
    <label for="intro_form_lname_2">Last name:</label><br>
    <input type="text" id="intro_form_lname_2" name="intro_form_lname_2"><br><br>
    <label for="intro_form_email_2">Email:</label><br>
    <input type="email" id="intro_form_email_2" name="intro_form_email_2"><br><br>
    <label for="intro_form_phone_2">Phone</label><br>
    <input type="tel" id="intro_form_phone_2" name="intro_form_phone_2"><br><br>
    {% include '@psu-ooe/button/button.twig' with {
      type: 'submit',
      label: 'Submit',
    } only %}
  </form>
{% endset %}
{% include '@psu-ooe/intro-form/intro-form.twig' with {
  "color": "beaver-blue",
  "images": [
    "<img src=\"https://via.placeholder.com/800x350\" width=\"800\" height=\"350\" alt=\"\">",
  ],
  "title": "Ready to Learn More?",
  "description": "Get the resources you need to make informed decisions about your education. Request information on this program and other programs of interest by completing this form.",
  "form": form
} only %}
