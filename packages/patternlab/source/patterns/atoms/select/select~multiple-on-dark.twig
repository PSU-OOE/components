{% set select %}
  <div data-dark>
    <select multiple name="select_multiple_on_dark" id="select_multiple_on_dark">
      <option>Yes</option>
      <option>No</option>
      <option>Maybe</option>
    </select>
    <label class="visually-hidden" for="select_multiple_on_dark">Demo select multiple on dark background</label>
  </div>
{% endset %}

{% include '@psu-ooe/callout/callout.twig' with {
  content: select,
  background: 'blue',
} only %}
