{% set wc_logo %}
  {% include '@atoms/sprite/sprite.twig' with { name: 'wc-mark' } only %}
{% endset %}

  {% set apply_now %}
    {% include '@ooe/cta/cta-block.twig' with {
      label: 'Primary CTA',
    } %}
  {% endset %}
  {% set request_info %}
    {% include '@ooe/cta/cta-block.twig' with {
      label: 'Secondary CTA',
      color: 'alt',
    } %}
  {% endset %}

{% set header %}
  {% include '@molecules/branding/branding.twig' with {
    url: '/',
    image: wc_logo,
    site_name: 'Penn State World Campus',
    tagline: 'A world of possibilities. Online.',
    aria_current: false,
  } only %}
  {% include '@ooe/cta/cta-group.twig' with {
    ctas: [apply_now, request_info]
  } only %}
{% endset %}

{% set navigation %}
  <ul>
    <li><a href="/">Home</a></li>
    <li><a href="/">About Us</a></li>
    <li><a href="/">Degrees and Certificates</a></li>
    <li><a href="/">Technical Support</a></li>
    <li><a href="/">Student Services</a></li>
  </ul>
{% endset %}
{% set before_content %}
  {% include '@ooe/breadcrumbs/breadcrumbs.twig' with {
    heading: 'Breadcrumbs',
    heading_level: 'h2',
    id: 'breadcrumbs-top',
    breadcrumbs: [
      { url: '/', text: 'Home' },
      { url: '/', text: 'Parent page' },
      { text: 'Current page' }
    ]
  } only %}
{% endset %}

