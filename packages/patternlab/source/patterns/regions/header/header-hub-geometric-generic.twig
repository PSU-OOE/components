{% embed '@regions/header/header.twig' %}
  {% block dogear_content %}
    <a class="header__branding" href="/">
      {% include '@molecules/sprite/sprite.twig' with { name: 'psu-mark' } only %}
      <span class="visually-hidden">Penn State Example Site</span>
    </a>
  {% endblock %}
  {% block content %}
    <div class="tagline">
      <span class="tagline__text">Secure, accessible, beautiful sites. <wbr><strong class="tagline__strong">Made&nbsp;Easy.</strong></span>
    </div>
  {% endblock %}
  {% block overflow %}
    <div class="overflow-gap"></div>
  {% endblock %}
{% endembed %}
