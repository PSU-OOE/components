{% embed '@regions/header/header.twig' %}
  {% block dogear_content %}
    <div class="region region--header-branding">
      <a class="branding-block" href="/">
        {% include '@molecules/sprite/sprite.twig' with { name: 'psu-mark' } only %}
        <span class="visually-hidden">Penn State Example Site</span>
      </a>
    </div>
  {% endblock %}
  {% block content %}
    <div class="region region--header-tagline">
      {% include '@molecules/tagline/tagline.twig' with {
        content: 'Secure, accessible, beautiful sites. <wbr><strong class="tagline__strong">Made&nbsp;Easy.</strong>'
      } only %}
    </div>
  {% endblock %}
{% endembed %}
