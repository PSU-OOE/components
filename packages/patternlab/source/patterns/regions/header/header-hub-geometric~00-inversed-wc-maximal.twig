        <div class="region region--utility">
          <ul style="margin:0;list-style-type: none; flex: 1; display: flex; flex-wrap: wrap; gap: 2rem;">
            <li><a href="#">A-Z Index</a></li>
            <li><a href="#">About</a></li>
            <li><a href="#">Calendar</a></li>
            <li><a href="#">Help and Support</a></li>
            <li>{% include '@psu-ooe/compact-search/compact-search.twig' with { orientation: 'horizontal', 'search_form': 'The form goes here.' } only %}</li>
          </ul>
        </div>
        <div class="region region--segmentation">
          <div style="display: flex;gap:.5rem;">
            <span style="white-space: nowrap;">Your Program:</span>
            {% include '@psu-ooe/button/button.twig' with {
              label: '<strong>Business Management Foundations</strong> (Graduate Certificate)',
              label_clamp: 1,
              color: 'keystone',
              size: 'compact',
            } only %}
          </div>
        </div>
      </div>
    </div>
  {% endblock %}
  {% block overflow %}
    <div style="background-color:white;height:.1rem;width:100%"></div>
    <div class="region region--segmentation-small">
      <p>Small segmentation</p>
    </div>
    <div class="region region--primary-nav" data-dark>
      {% include '@psu-ooe/horizontal-menu/horizontal-menu.twig' with {
        menu_name: 'Primary navigation menu',
        items: [
          { title: 'Home', url: '#', in_active_trail: true },
          { title: 'Finances', url: '#' },
          { title: 'Inclusion and Wellness', url: '#' },
          { title: 'Enrollment and Registration' },
          { title: 'Course Work and Success', url: '#' },
          { title: 'Involvement and Opportunities', url: '#' },
        ],
      } only %}
    </div>
    <div class="region region--primary-nav-mobile">
      Mobile primary nav
    </div>
  {% endblock %}
{% endembed %}
