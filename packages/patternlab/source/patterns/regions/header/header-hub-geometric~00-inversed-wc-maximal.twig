<style>
  .header-container {
    width: 100%;
    gap: 4rem;
    display: flex;
  }
  .header-container svg {
    color: var(--beaver-blue);
    height: 4rem;
  }
  .header-container__secondary {
    flex: auto;
    display: flex;
    flex-direction: column;
    align-items: flex-end;
    gap: 1.5rem;
  }

  .region--primary-nav {
    padding: 0 4rem;
  }
</style>
