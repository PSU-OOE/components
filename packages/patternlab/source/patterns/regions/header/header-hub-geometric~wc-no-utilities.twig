{% endset %}
{% set site_name = 'Penn State World Campus' %}
{% set how_to_apply %}
  {% include '@atoms/cta/cta-block.twig' with {
    label: 'How to Apply',
    url: '#',
    color: 'light-blue',
    compact: true,
  } only %}{% endset %}
{% set request_info %}
  {% include '@atoms/cta/cta-block.twig' with {
    label: 'Request Info',
