{% set logo %}
  {% include '@molecules/sprite/sprite.twig' with { name: 'wc-mark' } only %}
{% endset %}
{% set site_name = 'Penn State World Campus' %}
{% set how_to_apply %}
  {% include '@atoms/cta/cta-block.twig' with {
    label: 'How to Apply',
    url: '#',
    color: 'light-blue',
  } only %}{% endset %}
{% set request_info %}
  {% include '@atoms/cta/cta-block.twig' with {
    label: 'Request Info',
    url: '#',
    color: 'reversed',
  } only %}
{% endset %}

{% set content %}
  <div class="header__large">
    <div class="header__large-cta">
      {% include '@atoms/cta/cta-group.twig' with {
        ctas: [how_to_apply, request_info],
        expand_to_fit: true,
      } only %}
    </div>
  </div>
{% endset %}
{% set overflow %}
  <div class="header__small-cta">
    {% include '@atoms/cta/cta-group.twig' with {
      ctas: [how_to_apply, request_info],
      expand_to_fit: true,
    } only %}
  </div>
{% endset %}
{% include '@regions/header/header.twig' %}
