{% embed '@psu-ooe/header/header.twig' %}
  {% block dogear_content %}
    <div class="region region--branding">
      <div class="block block--system-branding-block">
        <a href="/">
          {% include '@psu-ooe/sprite/sprite.twig' with { name: 'wc-mark' } only %}
          <span class="visually-hidden">Penn State World Campus</span>
        </a>
      </div>
    </div>
  {% endblock %}
  {% block content %}
    <div class="tagline">
      <div class="tagline__text">
        <div class="wysiwyg">Secure, accessible, beautiful sites. <wbr><strong class="tagline__strong">Made&nbsp;Easy.</strong></div>
      </div>
    </div>
  {% endblock %}
{% endembed %}
