{% embed '@psu-ooe/header/header.twig' %}
  {% block dogear_content %}
    <div class="region region--branding">
      <div class="block block--system-branding-block">
        <a href="/">
          <svg class="sprite sprite--wc-mark" aria-hidden="true" viewBox="0 0 229 72">
            <use xlink:href="#wc-mark"></use>
          </svg>
          <span class="visually-hidden">Penn State World Campus</span>
        </a>
      </div>
    </div>
  {% endblock %}
  {% block content %}
    <p style="height:100%;padding:1rem;text-align:center;display:flex;align-items:center;justify-content:center;outline:.2rem dashed;outline-offset:-.4rem;width:100%;">
      Content area for the header.
    </p>
  {% endblock %}
  {% block overflow %}
    <p style="padding:1rem;text-align:center;display:flex;align-items:center;justify-content:center;outline:.2rem dashed;outline-offset:-.4rem;">Optional, overflow area for things that might not quite fit otherwise.</p>
  {% endblock %}
{% endembed %}
