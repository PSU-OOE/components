{% embed '@psu-ooe/footer/footer.twig' %}
  {% block top_first %}
    <a href="#" style="display:inline-block;  width: max(13rem, 100%);min-width: 3rem;max-width: 22.2rem;color:var(--beaver-blue)">
      <span class="visually-hidden">Penn State World Campus</span>
      {% include '@psu-ooe/sprite/sprite.twig' with { name: 'wc-mark', expand: true } only %}
    </a>
  {% endblock %}
  {% block top_second %}
    {% include '@psu-ooe/heading/heading.twig' with {
      level: 2,
      size: 'medium',
      content: 'A world of possibilities. <span class="text--contrasting">Online.</span>'
    } only %}
    {% include '@psu-ooe/vertical-menu/vertical-menu.twig' with {
      menu_name: 'Example',
      items: [
        { title: 'Finances', url: '#', below: [
          { title: 'Tuition and Costs', url: '#' }, { title: 'Scholarships', url: '#' }, { title: 'Financial Aid', url: '#' }, { title: 'Paying Your Bill', url: '#' }, { title: 'Financial Literacy', url: '#' },
        ]},
        { title: 'Inclusion and Wellness', url: '#', below: [
          { title: 'Diversity and Inclusion', url: '#' }, { title: 'Disabilities and Accomodations', url: '#' }, { title: 'Mental Health', url: '#' },
        ]},
        { title: 'Enrollment and Registration', url: '#', below: [
          { title: 'Course Planning', url: '#' },
        ]},
        { title: 'Course Work and Success', url: '#', below: [
          { title: 'Academic Advising', url: '#' },
        ]},
        { title: 'Involvement and Opportunities', url: '#', below: [
          { title: 'Career Resources', url: '#' },
        ]},
      ],
    } only %}

  {% endblock %}
  {% block top_third %}
    {% include '@psu-ooe/list/list.twig' with {
      items: [
        '<a href="#" style="display:inline-block;padding:.5rem 0;">A-Z Index</a>',
        '<a href="#" style="display:inline-block;padding:.5rem 0;">Calendar</a>',
        '<a href="#" style="display:inline-block;padding:.5rem 0;">Help and Support</a>',
        '<a href="#" style="display:inline-block;padding:.5rem 0;">Search</a>',
      ],
    } only %}
    <div>
      <span class="text text--weight-bold">
        Information For:
        <span class="visually-hidden"> specific groups</span>
      </span>
      {% include '@psu-ooe/list/list.twig' with {
        items: [
          '<a href="#" style="display:inline-block;padding:.25rem 0;">New and Recently Admitted Students</a>',
          '<a href="#" style="display:inline-block;padding:.25rem 0;">Returning Students</a>',
          '<a href="#" style="display:inline-block;padding:.25rem 0;">Graduating Students</a>',
          '<a href="#" style="display:inline-block;padding:.25rem 0;">Military Students</a>',
        ],
      } only %}
    </div>
  {% endblock %}
  {% block middle_first %}
    <h2 class="visually-hidden">Contact information</h2>
    <address>
      <div style="white-space: nowrap;color:var(--beaver-blue)">
        <span class="visually-hidden">phone</span>
        {% include '@psu-ooe/sprite/sprite.twig' with { name: 'phone' } only %}&nbsp;
        <a href="#"><span class="text text--size-msmall text--height-snug text--weight-semibold"> 814-865-5403</span></a>
      </div>
      <div style="white-space: nowrap;color:var(--beaver-blue)">
        <span class="visually-hidden">email</span>
        {% include '@psu-ooe/sprite/sprite.twig' with { name: 'envelope' } only %}&nbsp;
        <a href="#"><span class="text text--size-msmall text--height-snug text--weight-semibold">worldcampus@psu.edu</span></a>
      </div>
    </address>
  {% endblock %}
  {% block middle_second %}
    {% include '@psu-ooe/physical-address/physical-address.twig' with {
      name: 'The Pennsylvania State University',
      street: '128 Outreach Building',
      locality: 'University Park',
      state: 'PA',
      postcode: '16802',
    } only %}
  {% endblock %}
  {% block middle_third %}
    {% set social_icons = [] %}
    {% for icon, label in {instagram: 'Instagram', facebook: 'Facebook', linkedin: 'LinkedIn', twitter: 'Twitter', youtube: 'YouTube', flickr: 'Flickr', pinterest: 'Pinterest', blog: 'Blog'} %}
      {% set social_icon %}
        {% include '@psu-ooe/social-icon/social-icon.twig' with {
          color: 'dark',
          icon: icon,
          label: label,
          url: '#',
        } only %}
      {% endset %}
      {% set social_icons = social_icons|merge([social_icon]) %}
    {% endfor %}

    <div style="width:22rem">
      {% include '@psu-ooe/social-icons/social-icons.twig' with { social_icons: social_icons } only %}
    </div>
  {% endblock %}
  {% block colophon %}
    <h2 class="visually-hidden">Disclosures, policies, and copyright information</h2>
    <div style="display:flex;gap:2rem 2.5%">
      <a href="#" style="color:var(--beaver-blue);display:inline-block;min-width:10rem;width:10rem;height:fit-content;padding:1rem 0;">
        <span class="visually-hidden">Penn State</span>
        {% include '@psu-ooe/sprite/sprite.twig' with { name: 'psu-mark', expand: true } only %}
      </a>
      {% include '@psu-ooe/list/list.twig' with {
        orientation: 'horizontal',
        separator: 'dot',
        items: [
          '<a style="display:inline-block;padding:.5rem 0;" href="#"><span class="text text--size-3xsmall text--weight-semibold text--height-snug">Accessibility</span></a>',
          '<a style="display:inline-block;padding:.5rem 0;" href="#"><span class="text text--size-3xsmall text--weight-semibold text--height-snug">Equal Opportunity</span></a>',
          '<a style="display:inline-block;padding:.5rem 0;" href="#"><span class="text text--size-3xsmall text--weight-semibold text--height-snug">Nondiscrimination</span></a>',
          '<a style="display:inline-block;padding:.5rem 0;" href="#"><span class="text text--size-3xsmall text--weight-semibold text--height-snug">Privacy</span></a>',
          '<a style="display:inline-block;padding:.5rem 0;" href="#"><span class="text text--size-3xsmall text--weight-semibold text--height-snug">Consumer Information and Disclosures</span></a>',
          '<a style="display:inline-block;padding:.5rem 0;" href="#"><span class="text text--size-3xsmall text--weight-semibold text--height-snug">The Pennsylvania State University Â© 2024</span></a>',
        ]
      } only %}
    </div>
  {% endblock %}
{% endembed %}
