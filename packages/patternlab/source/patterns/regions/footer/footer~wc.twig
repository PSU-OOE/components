{% embed '@psu-ooe/footer/footer.twig' %}
  {% block top_first %}
    <div>No generic branding component :-(</div>
    <a href="#">
      <span class="visually-hidden">Penn State World Campus</span>
      {% include '@psu-ooe/sprite/sprite.twig' with { name: 'wc-mark' } only %}
    </a>
  {% endblock %}
  {% block top_second %}
    {% include '@psu-ooe/heading/heading.twig' with {
      level: 2,
      size: 'medium',
      content: 'A world of possibilities. <span class="text--contrasting">Online.</span>'
    } only %}
    {% include '@psu-ooe/vertical-menu/vertical-menu.twig' with {
      menu_name: 'Example',
      items: [
        { title: 'Home', url: '#' },
        { title: 'About Us', url: '/about-us', below: [
          { title: 'Our Story', url: '#' },
          { title: 'Mission', url: '#' },
          { title: 'Our Leadership', url: '#' },
          { title: 'Our Students', url: '#' },
          { title: 'Why World Campus', url: '#' },
          { title: 'News and Features', url: '#' },
          { title: 'Success Stories', url: '#' },
          { title: 'Employment', url: '#' },
        ]},
        { title: 'Degrees and Certificates', url: '#', below: [

        ]},
        { title: 'How Online Learning Works', url: '#', below: [
          { title: 'Online Learning Experience', url: '#' },
          { title: 'Student Services', url: '#' },
          { title: 'Dedication to Accessibility', url: '#' },
          { title: 'How it Works (FAQs)', url: '#' },
        ]},
        { title: 'Admissions', url: '#', below: [
          { title: 'How to Apply', url: '#' },
          { title: 'Returning Students', url: '#' },
          { title: 'Transfer Students', url: '#' },
          { title: 'International Students', url: '#' },
          { title: 'Academic Calendar', url: '#' },
          { title: 'Planning Resources', url: '#' },
        ]},
        { title: 'Tuition and Financial Aid', url: '#', below: [
          { title: 'Tuition and Costs', url: '#' },
          { title: 'Financial Aid', url: '#' },
          { title: 'Scholarships', url: '#' },
          { title: 'Payment Options', url: '#' },
        ]},
        { title: 'Military', url: '#', below: [
          { title: 'Education Benefits and Financial Aid', url: '#' },
          { title: 'Admissions and Transfer Credits', url: '#' },
          { title: 'Military Support', url: '#' },
          { title: 'What You Can Expect', url: '#' },
          { title: 'Awards and Affiliations', url: '#' },
          { title: 'Events', url: '#' },
          { title: 'Service Members', url: '#' },
          { title: 'Veterans', url: '#' },
        ]},
      ],
    } only %}

  {% endblock %}
  {% block top_third %}
    <p>Lists could use some variants, probably...</p>
    <ul>
      <li><a href="#">Contacts & Help</a></li>
      <li><a href="#">Events</a></li>
      <li><a href="#">On-Demand Webinars</a></li>
      <li><a href="#">AcademicCalendar</a></li>
    </ul>
    <div>
      <span>
        Information For:
        <span class="visually-hidden"> specific groups</span>
      </span>
      <ul>
        <li><a href="#">Current Students</a></li>
        <li><a href="#">Alumni and Friends</a></li>
        <li><a href="#">Education Alliance</a></li>
        <li><a href="#">Faculty and Staff</a></li>
      </ul>
    </div>
  {% endblock %}
  {% block middle_first %}
    <h2 class="visually-hidden">Contact information</h2>
    <div>
      <span class="visually-hidden">phone</span>
      {% include '@psu-ooe/sprite/sprite.twig' with { name: 'phone' } only %}
      <a href="#">814-865-5403</a>
    </div>
    <div>
      <span class="visually-hidden">email</span>
      {% include '@psu-ooe/sprite/sprite.twig' with { name: 'envelope' } only %}
      <a href="#">worldcampus@psu.edu</a>
    </div>
    <div>
      U.S. Toll Free:
      <a href="#">800-252-3592</a>
    </div>
    <div>
      Fax: 814-865-3290
    </div>
  {% endblock %}
  {% block middle_second %}
    <p>A semantic <a href="/patterns/organisms-address/index.html">address component</a>, yay!</p>
    {% include '@psu-ooe/address/address.twig' with {
      name: 'The Pennsylvania State University',
      street: '128 Outreach Building',
      locality: 'University Park',
      state: 'PA',
      postcode: '16802',
    } only %}
  {% endblock %}
  {% block middle_third %}
    {% set social_medias = {
      instagram: 'Instagram',
      facebook: 'Facebook',
      linkedin: 'LinkedIn',
      twitter: 'Twitter',
      youtube: 'YouTube',
      flickr: 'Flickr',
      pinterest: 'Pinterest',
      blog: 'Blog',
    } %}

    {% set social_icons = [] %}
    {% for icon, label in social_medias %}
      {% set social_icon %}
        {% include '@psu-ooe/social-icon/social-icon.twig' with {
          color: 'light',
          icon: icon,
          label: label,
          url: '#',
        } only %}
      {% endset %}
      {% set social_icons = social_icons|merge([social_icon]) %}
    {% endfor %}

    {% include '@psu-ooe/social-icons/social-icons.twig' with { social_icons: social_icons } only %}
  {% endblock %}
  {% block bottom %}
    <h2 class="visually-hidden">Disclosures, policies, and copyright information</h2>
    <p>Horizontal list variant probably needed.</p>
    <ul>
      <li>
        <a href="#">
          <span class="visually-hidden">Penn State</span>
          {% include '@psu-ooe/sprite/sprite.twig' with { name: 'psu-mark' } only %}
        </a>
      </li>
      <li><a href="#">Accessibility</a></li>
      <li><a href="#">Equal Opportunity</a></li>
      <li><a href="#">Nondiscrimination</a></li>
      <li><a href="#">Privacy</a></li>
      <li><a href="#">Consumer Information and Disclosures</a></li>
      <li><a href="#">The Pennsylvania State University Â© 2024</a></li>
    </ul>
  {% endblock %}
{% endembed %}
