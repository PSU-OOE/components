<style>
  {# Regions backport from wcprospect8 #}
  .region--utility-small-viewports{display:flex;gap:2rem;width:100%;height:100%;align-items:flex-end;justify-content:flex-end}@media(min-width: 950px){.region--utility-small-viewports{display:none}}.region--cta-small-viewports{display:flex;gap:clamp(0.5rem,1.25vw,1rem);padding:1.2rem 0}@media(min-width: 950px){.region--cta-small-viewports{display:none}}.region--cta-small-viewports:empty{padding:0.6rem 0}.region--utility-large-viewports{display:none}@media(min-width: 950px){.region--utility-large-viewports{display:flex;width:100%;gap:1.5rem;align-items:center;justify-content:flex-end}}@media(min-width: 950px)and (min-width: 1024px){.region--utility-large-viewports{gap:2rem}}@media(min-width: 950px){.region--utility-large-viewports+.region--cta-large-viewports{margin-left:3rem}}@media(min-width: 950px)and (min-width: 1024px){.region--utility-large-viewports+.region--cta-large-viewports{margin-left:4rem}}.region--cta-large-viewports{display:none}@media(min-width: 950px){.region--cta-large-viewports{display:flex;gap:clamp(0.5rem,1.25vw,1rem);margin-left:auto;white-space:nowrap}}.region--overflow-large-viewports{display:none}@media(min-width: 950px){.region--overflow-large-viewports{display:block}}.region--overflow-large-viewports:empty{padding:0.6rem 0}

  {# "System branding block" backport from wcprospect8 #}
  .block--system-branding-block .sprite{color:var(--beaver-blue);height:auto;width:13rem;min-width:3rem;max-width:100%}@media(min-width: 550px){.block--system-branding-block .sprite{width:18.5rem}}

  {# "Utility menu" backport from wcprospect8 #}
  .utility-menu{display:flex;justify-content:flex-end;align-items:center;gap:1.5rem;margin:0;padding:0;list-style-type:none}@media(min-width: 1024px){.utility-menu{gap:2rem}}.utility-menu__link{color:var(--text-color--light);text-decoration:none;font-family:var(--font-family--default);font-weight:var(--font-weight--semibold);font-size:var(--font-size--xsmall);line-height:var(--line-height--nospace);letter-spacing:var(--letter-spacing--medium)}@media(min-width: 1280px){.utility-menu__link{font-size:var(--font-size--msmall)}}

</style>
<style>
  .pl-c-main:has(.page-title) {
    padding: 0;
  }
    .basic-page-content {
        padding: 0 2rem;
    }
    @media screen and(min-width: 550px) {
        .basic-page-content {
            padding: 0 4rem;
        }
    }
</style>

{% embed '@psu-ooe/header/header.twig' %}

  {% set how_to_apply %}
    {% include '@psu-ooe/cta/cta-block.twig' with {
      label: 'How to Apply',
      url: '#',
      color: 'light-blue',
      compact: true,
    } only %}{% endset %}
  {% set request_info %}
    {% include '@psu-ooe/cta/cta-block.twig' with {
      label: 'Request Info',
      url: '#',
      color: 'reversed',
      compact: true,
    } only %}
  {% endset %}

  {% set search_horizontal %}
    {% include '@psu-ooe/compact-search/compact-search.twig' with {
      align: 'right',
      orientation: 'horizontal',
      search_form: 'The search form goes here.'
    } only %}
  {% endset %}

  {% set search_vertical %}
    {% include '@psu-ooe/compact-search/compact-search.twig' with {
      align: 'right',
      search_form: 'The search form goes here.'
    } only %}
  {% endset %}

  {% set horizontal_menu %}
    {% include '@psu-ooe/horizontal-menu/horizontal-menu.twig' with {
      menu_name: 'Primary navigation menu',
      items: [
        { title: 'Home', url: '#', in_active_trail: true },
        { title: 'About Us', url: '#' },
        { title: 'Degrees and Certificates', url: '#' },
        { title: 'How Online Learning Works' },
        { title: 'Admissions', url: '#' },
        { title: 'Tuition and Financial Aid', url: '#' },
        { title: 'Military', url: '#'},
      ],
    } only %}
  {% endset %}

  {% set vertical_menu %}
    {% include '@psu-ooe/vertical-menu/vertical-menu.twig' with {
      menu_name: 'Primary navigation menu',
      density: 'tight',
      items: [
        { title: 'Home', url: '#', in_active_trail: true },
        { title: 'About Us', url: '#' },
        { title: 'Degrees and Certificates', url: '#' },
        { title: 'How Online Learning Works' },
        { title: 'Admissions', url: '#' },
        { title: 'Tuition and Financial Aid', url: '#' },
        { title: 'Military', url: '#'},
      ],
    } only %}
  {% endset %}

  {% set menu %}
    {% include '@psu-ooe/compact-menu/compact-menu.twig' with {
      align: 'right',
      menu: vertical_menu
    } only %}
  {% endset %}

  {% block dogear_content %}
    <div class="region region--branding">
      <div class="block block--system-branding-block">
        <a href="/">
          <svg class="sprite sprite--wc-mark" aria-hidden="true" viewBox="0 0 229 72">
            <use xlink:href="#wc-mark"></use>
          </svg>
          <span class="visually-hidden">Penn State World Campus</span>
        </a>
      </div>
    </div>
  {% endblock %}
  {% block content %}
    <div class="region region--utility-small-viewports">
      {{ search_vertical }}
      {{ menu }}
    </div>
    <div class="region region--utility-large-viewports">
      <ul class="utility-menu" role="list">
        <li class="utility-menu__item"><a class="utility-menu__link" href="#">Current Students</a></li>
        <li class="utility-menu__item"><a class="utility-menu__link" href="#">Alumni</a></li>
        <li class="utility-menu__item"><a class="utility-menu__link" href="#">Events</a></li>
      </ul>
      {{ search_horizontal }}
    </div>
    <div class="region region--cta-large-viewports">
      {% apply spaceless %}
        {{ how_to_apply }}
        {{ request_info }}
      {% endapply %}
    </div>
  {% endblock %}
  {% block overflow %}
    <div class="region region--cta-small-viewports">
      {% apply spaceless %}
        {% include '@psu-ooe/cta/cta-group.twig' with {
          ctas: [how_to_apply, request_info],
          expand_to_fit: true,
        } only %}
      {% endapply %}
    </div>
    <div class="region region--overflow-large-viewports">
      {{ horizontal_menu }}
    </div>
  {% endblock %}
{% endembed %}

{% include '@psu-ooe/page-title-banner-image/page-title-banner-image.twig' with {
