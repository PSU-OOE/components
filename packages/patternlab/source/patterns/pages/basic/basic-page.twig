    </picture>
  {% endset %}

  {% include '@psu-ooe/overlay/overlay.twig' with {
    image: overlay_image,
    content: overlay_content,
    overlay_bg_color: "blue-gradient",
    column_width: "normal",
    overlay_width: "wide"
  } %}
</main>
{% set footer_top %}
  <div class="basic-page-content" style="max-width: 119rem;margin:auto;display:flex;min-height:25rem;">
    Footer top content
  </div>
{% endset %}
{% set footer_bottom %}
  <div class="basic-page-content" style="color:#fff;max-width: 119rem;margin:auto;display:flex;min-height:12rem;">
    Footer bottom content
  </div>
{% endset %}
<footer>
  {% include '@psu-ooe/background-container/background-container.twig' with {
    color: 'light-grey',
    background_image: 'hub-geometric:topleft',
    content: footer_top,
  } only %}
  {% include '@psu-ooe/background-container/background-container.twig' with {
    color: 'beaver-blue',
    background_image: 'hub-geometric:bottomright',
    content: footer_bottom,
  } only %}
</footer>

