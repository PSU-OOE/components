  {% include '@psu-ooe/background-container/background-container.twig' with {
    color: 'white',
    content: page_content,
  } only %}

  {% set tile_nav_content %}
    <div class="basic-page-content" style="margin: auto;display: flex; flex-direction: column; max-width: 119rem;">
      {% set tile %}
        {% include '@psu-ooe/navigation-tile/navigation-tile.twig' with {
          link: {
            url: '#',
            label: 'Navigation Title',
          },
          description: "The Office of the Vice Provost for Online Education is led by Dr. Renata S. Engel and is responsible for all aspects of Penn State World Campus.",
        } only %}

      {% endset %}
      {% set tile2 %}
        {% include '@psu-ooe/navigation-tile/navigation-tile.twig' with {
          link: {
            url: '#',
            label: 'Navigation Title with a really long title that will go to two lines eventually',
          },
          description: "The Office of the Vice Provost for Online Education is led by Dr. Renata S. Engel and is responsible for all aspects of Penn State World Campus.",
        } only %}
