<div class="image-navigation-tile" style="background-image: url('{{ image }}');">
  <a class="image-navigation-tile__link" href="{{ link.url }}">
    <span class="visually-hidden">{{ link.label }}</span>
  </a>
    <div class="image-navigation-tile__content">
      <div class="image-navigation-tile__title" aria-hidden="true">
        <span>{{ link.label }}{% include '@atoms/sprite/sprite.twig' with { name: 'fas-chevron-right' } only %}</span>
      </div>
    </div>
</div>

