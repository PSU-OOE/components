{% if bios|length > 1 %}
    <ul class="bio-collection">
      {% for bio in bios %}
        <li class="bio-collection__bio">
          {% include '@organisms/bio/bio.twig' with bio only %}
        </li>
      {% endfor %}
    </ul>
{% else %}
  {% include '@organisms/bio/bio.twig' with bios[0] only %}
{% endif %}
